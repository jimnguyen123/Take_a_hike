		<script type="text/javascript">
        
            function initialize() {
                // Basic options for a simple Google Map
                // For more options see: https://developers.google.com/maps/documentation/javascript/reference#MapOptions
				//var myLatlng = new google.maps.LatLng(32.936714,-117.041564);
				var myLatlng = new google.maps.LatLng(33.020179,-116.37199402);
                var mapOptions = {
                    // How zoomed in you want the map to start at (always required)
                    zoom: 8,
					// Mark location
					//markers: [ { address:"myLatlng", size:"mid", color:"white", image: "http://www.google.com/mapfiles/marker.png"|(32.936714,-117.041564)}],
					//markers: [],

                    // The latitude and longitude to center the map (always required)
                    center: myLatlng, // Poway
                    disableDefaultUI: true,

                    // How you would like to style the map. 
                    // This is where you would paste any style found on Snazzy Maps.
                    styles: [	{		featureType:"all",		elementType:"all",		stylers:[		{			invert_lightness:true		},		{			saturation:10		},		{			lightness:30		},		{			gamma:0.5		},		{			hue:"#009AD4"		}		]	}	]
                }
                disableDefaultUI: true
			
				
				var map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

				var infowindow = new google.maps.InfoWindow();

				for( var i =0; i < trails.length; i++){
					var hike = trails[i];
					var myLatLng = new google.maps.LatLng(hike[1], hike[2]);
					var marker = new google.maps.Marker({
						position: myLatLng,
						map: map,
						title: hike[0]
					});
					google.maps.event.addListener(marker, 'click', (function(mm, tt) {
    					return function() {
        					infowindow.setContent(tt);
        					infowindow.open(map, mm);
   						}
					})(marker, hike[3]));

				}

            }

            var trails = [
					['Mt. Woodson Trail, CA', 33.013972, -116.955385, 
					'<div id="content">'+
					'<div id="siteNotice">'+
					'</div>'+
					'<h3 id="firstHeading" class="firstHeading">Mt. Woodson Trail, CA</h3>'+
					'<div id="bodyContent">'+
					'<p> 13000 Gregg Street, Suite C ' +
					'Poway, CA 92064 </p>'+ 
					'<a href="http://www.google.com"> Test link </a>' +
					'</div>'+
					'</div>'],

					['Cowles Mountain Trail', 32.7872771, -117.031612, 
					'<div id="content">'+
					'<div id="siteNotice">'+
					'</div>'+
					'<h3 id="firstHeading" class="firstHeading">Cowles Mountain Trail</h3>'+
					'<div id="bodyContent">'+
					'<p> 13000 Gregg Street, Suite C ' +
					'Poway, CA 92064 </p>'+ 
					'<a href="http://www.google.com"> Test link </a>' +
					'</div>'+
					'</div>']
			];
			
			
            function loadScript() {
            	var script = document.createElement('script');
          		script.type = 'text/javascript';
          		script.src = "http://maps.googleapis.com/maps/api/js?key=AIzaSyBBjR3MXhPjJImUhQEpD8tyU8yQ4xZ965A&sensor=TRUE_OR_FALSE&callback=initialize";
          		document.body.appendChild(script);
            }
            window.onload = loadScript;
        </script>
		<script type="text/javascript">
